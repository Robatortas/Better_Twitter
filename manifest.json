{
    "manifest_version": 3,
    "name": "Better Twitter",
    "description": "Revert back to your favorite bird app.",
    "version": "1.0",
    "action": {
      "default_popup": "bt.html",
      "default_icon": "res/bt.png"
    },
    "content_scripts": [
      {
        "js": ["contents/bt.js"],
        "matches": [
          "https://twitter.com/*",
          "https://x.com/*"
        ]
      }
    ]
  }